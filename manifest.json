{
    "name": "RESTer",
    "version": "0.2.0",
    "icons": {
        "16": "/app/images/icons/icon_16.png",
        "48": "/app/images/icons/icon_48.png",
        "128": "/app/images/icons/icon_128.png"
    },

    "description": "simple approach for service testing",
    "permissions": ["http://*/*/", "https://*/*/", "storage", "webview"],
    "app": {
        "background": {
            "scripts": [
            "app/background.js"
            ],
            "persistent": false
        }
    },

    "manifest_version": 2
}